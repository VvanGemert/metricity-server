script src="//cdnjs.cloudflare.com/ajax/libs/jquery/2.1.1/jquery.min.js" type="text/javascript"
script src="http://code.highcharts.com/highcharts.js" type="text/javascript"
script src="http://code.highcharts.com/highcharts-more.js" type="text/javascript"
script src="http://code.highcharts.com/modules/exporting.js" type="text/javascript"
script src="/js/application.js" type="text/javascript"

h1= "Testing"
#area

#area2

javascript:
  $(function () {
        var date = new Date();
        var timeZoneOffset = date.getTimezoneOffset();
        
        Highcharts.setOptions({
          global: {
            timezoneOffset: timeZoneOffset
          }
        });
    
        $.getJSON('/data2.json', function(data) {
    
        	$('#area').highcharts({
    		    chart: {
    		        type: 'area',
    		        zoomType: 'x'
    		    },
    		    
    		    title: {
    		        text: 'Memory Usage'
    		    },
    		
    		    xAxis: {
    		        type: 'datetime',
    		        minRange: 1 * 24 * 360000
    		    },
    		    
    		    yAxis: {
    		        title: {
    		            text: 'Memory in MB'
    		        }
    		    },
            plotOptions: {
                area: {
                    stacking: 'normal',
                    lineColor: '#666666',
                    lineWidth: 0,
                    marker: {
                        enabled: false
                    }
                }
            },

    		    
    		    legend: {
    		        enabled: true
    		    },
    		
    		    series: data	
    		});
      });
      
      $.getJSON('/data.json', function(data) {
    
        	$('#area2').highcharts({
        	  
    		    chart: {
    		        type: 'area',
    		        zoomType: 'x'
    		    },
    		    
    		    title: {
    		        text: 'CPU Usage'
    		    },
    		
    		    xAxis: {
    		        type: 'datetime',
    		        minRange: 1 * 24 * 36000
    		    },
    		    
    		    yAxis: {
    		        title: {
    		            text: 'CPU in %'
    		        }
    		    },
            plotOptions: {
                area: {
                    stacking: 'normal',
                    lineColor: '#666666',
                    lineWidth: 0,
                    marker: {
                        enabled: false
                    }
                }
            },

    		    
    		    legend: {
    		        enabled: true
    		    },
    		
    		    series: data	
    		});
      });

    });